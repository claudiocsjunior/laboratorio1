#ifndef JOGADOR_H_
#define JOGADOR_H_
#include <iostream>
#include <string>
using namespace std;

class Jogador {
private:
	string m_nome;
	int pontuacao;
public:
	Jogador();
	virtual ~Jogador();
	const string& getNome() const;
	int getPontuacao() const;
	void setPontuacao(int pontuacao);
};

#endif /* JOGADOR_H_ */
